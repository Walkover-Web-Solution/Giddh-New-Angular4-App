  "build": {
    "appId": "com.electron.angular-electron-dream-starter",
    "directories": {
      "buildResources": "src/resources",
      "output": "packages",
      "app": "dist"
    },
    "mac": {
      "category": "public.app-category.graphics-design",
      "target": [
        "dmg"
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "snap"
      ]
    },
    "asar": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AngularClass/angular-starter.git"
  },
  "bugs": {
