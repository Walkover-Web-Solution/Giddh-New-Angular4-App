{
    "appId": "com.giddh.prod",
    "electronVersion": "11.2.3",
    "publish": [
        {
            "provider": "s3",
            "bucket": "giddh-app-builds-test"
        }
    ],
    "directories": {
        "buildResources": "resources",
        "output": "./../electrongiddh-packages"
    },
    "afterSign": "./../../../electron-sign/notarize.js",
    "mac": {
        "category": "public.app-category.finance",
        "target": [
            "dmg",
            "zip"
        ],
        "identity": "Walkover Web Solutions Private Limited (F3U6Z5L2EJ)",
        "type": "distribution",
        "entitlements": "./../../../electron-sign/default.entitlements.mas.plist",
        "entitlementsInherit": "./../../../electron-sign/default.entitlements.mas.inherit.plist",
        "provisioningProfile": "./../../../electron-sign/embedded.provisionprofile",
        "icon": "icon.png",
        "hardenedRuntime": true,
        "gatekeeperAssess": false
    },
    "dmg": {
        "sign": false
    },
    "mas": {
        "entitlements": "./../../../electron-sign/default.entitlements.mas.plist",
        "entitlementsInherit": "./../../../electron-sign/default.entitlements.mas.inherit.plist",
        "provisioningProfile": "./../../../electron-sign/embedded.provisionprofile"
    },
    "linux": {
        "icon": "icon.png",
        "maintainer": "Walkover Technologies Pvt Ltd",
        "target": [
            "AppImage",
            "snap",
            "zip"
        ]
    },
    "win": {
        "target": [
            "nsis",
            "nsis-web",
            "msiWrapped",
            "zip"
        ],
        "icon": "./resources/icon.ico",
        "publisherName": "",
        "legalTrademarks": "",
        "signAndEditExecutable": false
    },
    "nsis": {
        "oneClick": true,
        "createDesktopShortcut": true,
        "createStartMenuShortcut": true,
        "menuCategory": "Accounting",
        "shortcutName": "Giddh",
        "perMachine": true,
        "include": "./../../../electron-sign/installer.nsh"
    },
    "msiWrapped": {
        "oneClick": true,
        "createDesktopShortcut": true,
        "createStartMenuShortcut": true,
        "menuCategory": "Accounting",
        "shortcutName": "Giddh",
        "perMachine": true,
        "wrappedInstallerArgs": "./../../../electron-sign/installer.nsh"
    },
    "asar": true
}