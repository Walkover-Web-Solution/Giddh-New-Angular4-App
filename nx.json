{
    "npmScope": "giddh-workspaces",
    "implicitDependencies": {
        "angular.json": "*",
        "package.json": {
            "dependencies": "*",
            "devDependencies": "*"
        },
        ".eslintrc.json": "*",
        "tsconfig.json": "*",
        "nx.json": "*"
    },
    "affected": {
        "defaultBase": "master"
    },
    "cli": {
        "defaultCollection": "@nrwl/angular",
        "packageManager": "npm"
    },
    "tasksRunnerOptions": {
        "default": {
            "runner": "@nrwl/nx-cloud",
            "options": {
                "cacheableOperations": [
                    "build",
                    "test",
                    "lint",
                    "e2e"
                ],
                "accessToken": "MDQ1MjA2M2YtNjgxNy00NWQ0LWE2ODItMGFiZWIxY2Y4MTdjfHJlYWQtd3JpdGU="
            }
        }
    },
    "generators": {
        "@nrwl/angular": {
            "application": {
                "linter": "eslint"
            },
            "library": {
                "linter": "eslint"
            },
            "storybook-configuration": {
                "linter": "eslint"
            }
        },
        "@nrwl/angular:application": {
            "style": "scss",
            "linter": "eslint",
            "unitTestRunner": "jest",
            "e2eTestRunner": "cypress"
        },
        "@nrwl/angular:library": {
            "linter": "eslint",
            "unitTestRunner": "jest"
        },
        "@nrwl/angular:component": {
            "style": "scss"
        }
    }
}
