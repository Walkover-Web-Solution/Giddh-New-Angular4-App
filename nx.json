{
    "npmScope": "giddh-workspaces",
    "implicitDependencies": {
        "angular.json": "*",
        "package.json": "*",
        "tsconfig.json": "*",
        "tslint.json": "*",
        "nx.json": "*"
    },
    "affected": {
        "defaultBase": "master"
    },
    "cli": {
        "defaultCollection": "@nrwl/angular"
    },
    "generators": {
        "@nrwl/angular:application": {
            "unitTestRunner": "jest",
            "e2eTestRunner": "cypress"
        },
        "@nrwl/angular:library": {
            "unitTestRunner": "jest"
        }
    }
}
