                                "mergeRules": {
                                    "externals": "replace"
                                }
                            },
                            "baseHref": "https://stage.giddh.com/",
                            "fileReplacements": [
                                {
                                    "replace": "apps/web-giddh/src/environments/environment.ts",
                                    "with": "apps/web-giddh/src/environments/environment.stage.ts"
                                }
                            ],
                            "index": {
                                "input": "apps/web-giddh/src/index.stage.html",
                                "output": "/index.html"
