                                "input": "apps/web-giddh/src/index.test.html",
                                "output": "index.html"
                            },
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": true,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "10mb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-builders/custom-webpack:dev-server",
                    "options": {
                        "host": "localhost",
                        "baseHref": "http://localhost:3000",
                        "port": 3000,
                        "browserTarget": "web-giddh:build"
                    },
                    "configurations": {
                        "prod": {
                            "browserTarget": "web-giddh:build:prod"
                        }
                    }
                },
