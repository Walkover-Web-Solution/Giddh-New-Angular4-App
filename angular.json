        "web-giddh": {
            "root": "apps/web-giddh/",
            "sourceRoot": "apps/web-giddh/src",
            "projectType": "application",
            "prefix": "foo",
            "schematics": {
                "@nrwl/angular:component": {
                    "style": "scss"
                }
            },
            "architect": {
                "build": {
                    "builder": "@angular-builders/custom-webpack:browser",
                    "options": {
                        "baseHref": "https://beta.giddh.com",
                        "allowedCommonJsDependencies": [
                            "file-saver",
                            "angular2-text-mask",
                            "ngrx-store-localstorage",
                            "css-element-queries",
                            "jspdf",
                            "jspdf-autotable",
                            "webfontloader",
                            "ng2-order-pipe",
                            "jquery"
                        ],
                        "outputPath": "dist/apps/web-giddh",
                        "index": "apps/web-giddh/src/index.html",
                        "main": "apps/web-giddh/src/main.ts",
                        "polyfills": "apps/web-giddh/src/polyfills.ts",
                        "tsConfig": "apps/web-giddh/tsconfig.app.json",
                        "assets": [
                            {
                                "glob": "**/*",
                                "input": "apps/web-giddh/src/assets/",
                                "output": "/assets/"
                            },
                            {
                                "glob": "**/*",
                                "input": "apps/web-giddh/src/meta/",
                                "output": "/"
                            },
                            {
                                "glob": "favicon.ico",
                                "input": "apps/web-giddh/src/",
                                "output": "/"
                            },
                            {
                                "glob": "apple-developer-domain-association.txt",
                                "input": "apps/web-giddh/src/.well-known/",
                                "output": "/.well-known/"
                            },
                            {
                                "glob": "**/*",
                                "input": "cypress/",
                                "output": "/cypress/"
                            }
                        ],
                        "styles": [
                            "apps/web-giddh/src/assets/css/ngx-bootstrap/bs-datepicker.css",
                            "apps/web-giddh/src/assets/css/font-awesome.css",
                            "apps/web-giddh/src/assets/fonts/icomoon/icomoon.css",
                            "apps/web-giddh/src/assets/css/ledgerfont/ledgerfont.css",
                            "apps/web-giddh/src/assets/css/style-1.scss",
                            "apps/web-giddh/src/assets/css/style-2.scss",
                            "apps/web-giddh/src/assets/css/ladda-themeless.min.css",
                            "apps/web-giddh/src/assets/css/perfect-scrollbar.component.css",
                            "apps/web-giddh/src/assets/css/toastr.css"
                        ],
                        "scripts": [
                            "apps/web-giddh/src/assets/js/jquery-1.11.3.min.js",
                            "apps/web-giddh/src/assets/js/lodash.min.js",
                            "node_modules/bootstrap-daterangepicker/moment.min.js",
                            "node_modules/bootstrap-daterangepicker/daterangepicker.js"
                        ],
                        "customWebpackConfig": {
                            "path": "apps/web-giddh/webpack.partial.js",
                            "mergeRules": {
                                "externals": "replace"
                            }
                        }
                    },
                    "configurations": {
                        "local": {
                            "outputPath": "dist/apps/web-giddh-local",
                            "customWebpackConfig": {
                                "path": "apps/web-giddh/webpack.local.partial.js",
                                "mergeRules": {
                                    "externals": "replace"
                                }
                            },
                            "baseHref": "https://beta.giddh.com",
                            "fileReplacements": [
                                {
                                    "replace": "apps/web-giddh/src/index.html",
                                    "with": "apps/web-giddh/src/index.local.html"
                                },
                                {
                                    "replace": "apps/web-giddh/src/environments/environment.ts",
                                    "with": "apps/web-giddh/src/environments/environment.local.ts"
                                }
                            ],
                            "index": {
                                "input": "apps/web-giddh/src/index.local.html",
                                "output": "index.html"
                            },
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "10mb"
                                }
                            ]
                        },
                        "stage": {
                            "outputPath": "dist/apps/web-giddh-stage",
                            "customWebpackConfig": {
                                "path": "apps/web-giddh/webpack.stage.partial.js",
                                "mergeRules": {
                                    "externals": "replace"
                                }
                            },
                            "baseHref": "https://stage.giddh.com/",
                            "fileReplacements": [
                                {
                                    "replace": "apps/web-giddh/src/index.html",
                                    "with": "apps/web-giddh/src/index.stage.html"
                                },
                                {
                                    "replace": "apps/web-giddh/src/environments/environment.ts",
                                    "with": "apps/web-giddh/src/environments/environment.stage.ts"
                                }
                            ],
                            "index": {
                                "input": "apps/web-giddh/src/index.stage.html",
                                "output": "index.html"
                            },
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": true,
                            "buildOptimizer": true
                        },
                        "prod": {
                            "outputPath": "dist/apps/web-giddh-prod",
                            "customWebpackConfig": {
                                "path": "apps/web-giddh/webpack.prod.partial.js",
                                "mergeRules": {
                                    "externals": "replace"
                                }
                            },
                            "baseHref": "https://beta.giddh.com/",
                            "fileReplacements": [
                                {
                                    "replace": "apps/web-giddh/src/index.html",
                                    "with": "apps/web-giddh/src/index.prod.html"
                                },
                                {
                                    "replace": "apps/web-giddh/src/environments/environment.ts",
                                    "with": "apps/web-giddh/src/environments/environment.prod.ts"
                                }
                            ],
                            "index": {
                                "input": "apps/web-giddh/src/index.prod.html",
                                "output": "index.html"
                            },
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "10mb"
                                }
                            ]
                        },
                        "test": {
                            "outputPath": "dist/apps/web-giddh-test",
                            "customWebpackConfig": {
                                "path": "apps/web-giddh/webpack.test.partial.js",
                                "mergeRules": {
                                    "externals": "replace"
                                }
                            },
                            "baseHref": "https://test.giddh.com/",
                            "fileReplacements": [
                                {
                                    "replace": "apps/web-giddh/src/index.html",
                                    "with": "apps/web-giddh/src/index.test.html"
                                },
                                {
                                    "replace": "apps/web-giddh/src/environments/environment.ts",
                                    "with": "apps/web-giddh/src/environments/environment.test.ts"
                                }
