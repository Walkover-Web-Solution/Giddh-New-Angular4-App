{
    "cli": {
        "analytics": false
    },
    "version": 1,
    "projects": {
        "electron-giddh": {
            "root": "apps/web-giddh/",
            "sourceRoot": "apps/web-giddh/src",
            "projectType": "application",
            "prefix": "foo",
            "schematics": {},
            "architect": {
                "build": {
                    "builder": "@angular-builders/custom-webpack:browser",
                    "options": {
                        "index": "apps/web-giddh/src/index.html",
                        "main": "apps/web-giddh/src/main.electron.ts",
                        "polyfills": "apps/web-giddh/src/polyfills.ts",
                        "tsConfig": "apps/web-giddh/tsconfig.app.json",
                        "assets": [
                            {
                                "glob": "**/*",
                                "input": "apps/web-giddh/src/assets/",
                                "output": "/assets/"
                            },
                            {
                                "glob": "**/*",
                                "input": "apps/web-giddh/src/meta/",
                                "output": "/"
                            },
                            {
                                "glob": "favicon.ico",
                                "input": "apps/web-giddh/src/",
                                "output": "/"
                            },
                            {
                                "glob": "**/*",
                                "input": "apps/electron-giddh/src/",
                                "output": ""
                            }
                        ],
                        "styles": [
                            "apps/web-giddh/src/assets/css/style-1.scss",
                            "apps/web-giddh/src/assets/css/style-2.scss",
                            "node_modules/intl-tel-input/build/css/intlTelInput.css",
                            "node_modules/froala-editor/css/froala_editor.pkgd.min.css",
                            "node_modules/froala-editor/css/froala_style.min.css",
                            "node_modules/node_modules/tributejs/dist/tribute.css",
                           "node_module/froala-editor/css/themes/gray.min.css"
                        ],
                        "scripts": [
                            "node_modules/bootstrap-daterangepicker/daterangepicker.js",
                            "apps/web-giddh/src/assets/js/intl-tel-input.js",
                            "node_modules/froala-editor/js/froala_editor.pkgd.min.js",
                            "node_modules/tributejs/dist/tribute.js"
                        ]
                    },
                    "configurations": {
                        "local": {
                            "outputPath": "dist/apps/web-giddh",
                            "customWebpackConfig": {
                                "path": "apps/web-giddh/webpack.electron.partial.js",
                                "mergeRules": {
                                    "externals": "replace"
                                }
                            },
                            "fileReplacements": [
                                {
                                    "replace": "apps/web-giddh/src/environments/environment.ts",
                                    "with": "apps/web-giddh/src/environments/environment.local.electron.ts"
                                }
                            ],
                            "index": {
                                "input": "apps/web-giddh/src/index.local.html",
                                "output": "/index.html"
                            },
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": false,
                            "extractLicenses": false,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "25mb",
                                    "maximumError": "25mb"
                                }
                            ]
                        },
                        "stage": {
                            "outputPath": "dist/apps/web-giddh",
                            "customWebpackConfig": {
                                "path": "apps/web-giddh/webpack.stage.partial.js",
                                "mergeRules": {
                                    "externals": "replace"
                                }
                            },
                            "fileReplacements": [
                                {
                                    "replace": "apps/web-giddh/src/environments/environment.ts",
                                    "with": "apps/web-giddh/src/environments/environment.stage.ts"
                                }
                            ],
                            "index": {
                                "input": "apps/web-giddh/src/index.stage.html",
                                "output": "/index.html"
                            },
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": false,
                            "vendorChunk": true,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "25mb",
                                    "maximumError": "25mb"
                                }
                            ]
                        },
                        "prod": {
                            "outputPath": "dist/apps/web-giddh",
                            "customWebpackConfig": {
                                "path": "apps/web-giddh/webpack.electron.prod.partial.js",
                                "mergeRules": {
                                    "externals": "replace"
                                }
                            },
                            "index": {
                                "input": "apps/web-giddh/src/index.electron.prod.html",
                                "output": "/index.html"
                            },
                            "fileReplacements": [
                                {
                                    "replace": "apps/web-giddh/src/environments/environment.ts",
                                    "with": "apps/web-giddh/src/environments/environment.prod.electron.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "namedChunks": true,
                            "aot": true,
                            "extractLicenses": false,
                            "vendorChunk": true,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "25mb",
                                    "maximumError": "25mb"
                                }
                            ]
                        },
                        "test": {
                            "outputPath": "dist/apps/web-giddh",
                            "customWebpackConfig": {
                                "path": "apps/web-giddh/webpack.electron.partial.js",
                                "mergeRules": {
                                    "externals": "replace"
                                }
                            },
                            "fileReplacements": [
                                {
                                    "replace": "apps/web-giddh/src/environments/environment.ts",
                                    "with": "apps/web-giddh/src/environments/environment.test.ts"
                                }
                            ],
                            "index": {
                                "input": "apps/web-giddh/src/index.test.html",
                                "output": "/index.html"
                            },
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "namedChunks": true,
                            "aot": true,
                            "extractLicenses": false,
                            "vendorChunk": true,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "25mb",
                                    "maximumError": "25mb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-builders/custom-webpack:dev-server",
                    "options": {
                        "host": "localhost",
                        "port": 4200,
                        "browserTarget": "electron-giddh:build:local",
                        "customWebpackConfig": {
                            "path": "apps/web-giddh/webpack.electron.partial.js",
                            "mergeRules": {
                                "externals": "replace"
                            }
                        }
                    },
                    "configurations": {
                        "prod": {
                            "browserTarget": "electron-giddh:build:prod"
                        }
                    }
                }
            }
        },
        "libs": {
            "root": "libs",
            "sourceRoot": "libs",
            "projectType": "library",
            "prefix": "foo",
            "architect": {
                "lint": {
                    "builder": "@angular-builders/custom-webpack:tslint",
