{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "foo",
      "camelCase"
    ],
    "component-selector": [
      false,
      "element",
      "foo",
      "kebab-case"
    ],
    "no-inferrable-types": false,
    "prefer-const": false,
    "nx-enforce-module-boundaries": [
      false
    ]
  }
}
